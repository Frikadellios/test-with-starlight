    "@types/html": "^1.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.3.0",
    "@types/three": "^0.167.1",
    "@types/web": "^0.0.154",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "rehype-autolink-headings": "^7.1.0",
    "unplugin-auto-import": "^0.18.2"
  },
  "peerDependencies": {
    "typescript": "^5.5.4"
  },
  "overrides": {
    "astro-expressive-code": "latest"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.2",
    "@astrojs/mdx": "^3.1.3",
    "@astrojs/react": "^3.6.2",
    "@astrojs/starlight": "^0.25.5",
    "@astrojs/ts-plugin": "^1.9.3",
    "@fontsource-variable/inter": "^5.0.20",
    "@nanostores/persistent": "^0.10.2",
    "@nanostores/react": "^0.7.3",
    "@react-three/drei": "^9.109.5",
