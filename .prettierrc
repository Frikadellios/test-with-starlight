{
    "useTabs": true,
    "singleQuote": true,
    "trailingComma": "none",
    "semi": false,
    "printWidth": 100,
    "overrides": [
       {
         "files": "*.svg",
         "options": {
           "parser": "html"
         }
       },
       {
         "files": "*.astro",
         "options": {
           "parser": "astro"
         }
       }
     ],
    "plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
    "pluginSearchDirs": false
 }
